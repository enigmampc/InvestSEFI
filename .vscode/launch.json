  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "main",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/main.js",
      "preLaunchTask": "tsc: build - tsconfig.json"
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "decrypt",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/decrypt.js",
      "preLaunchTask": "tsc: build - tsconfig.json"
    }
  ]
}
